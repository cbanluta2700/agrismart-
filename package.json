{
  "name": "agrismart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun run sync-check && node server.js",
    "build": "bun run sync-check && next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "sync-check": "bun run scripts/check-ecosystem-sync.ts",
    "sync-check:watch": "chokidar \"src/**/*.{ts,tsx}\" -c \"bun run sync-check\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "initialize-optimizations": "curl -X POST http://localhost:3000/api/admin/initialize-optimizations -H \"x-api-key: $ADMIN_API_KEY\"",
    "generate-mock-data": "node scripts/generate-mock-data.js",
    "start-mock-api": "node scripts/mock-api-server.js",
    "demo": "npm run generate-mock-data && npm run start-mock-api",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy",
    "db:deploy": "node scripts/deploy-db.js",
    "vercel-build": "npm run prisma:generate && next build",
    "vercel-postbuild": "npm run prisma:deploy"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@modelcontextprotocol/server-memory": "^0.6.2",
    "@prisma/client": "^5.9.1",
    "@redis/client": "^1.5.13",
    "@sentry/nextjs": "^7.100.1",
    "@socket.io/redis-adapter": "^8.2.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/ini": "^4.1.1",
    "@types/nodemailer": "^6.4.17",
    "@vercel/analytics": "^1.5.0",
    "@vercel/edge-config": "^1.4.0",
    "@vercel/sdk": "^1.3.1",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^3.0.2",
    "bun": "^1.0.26",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.7",
    "ini": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.14.0",
    "next": "14.1.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.15.1",
    "recoil": "^0.7.7",
    "swr": "^2.2.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.17",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vercel/node": "^5.1.10",
    "autoprefixer": "^10.4.17",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prisma": "^5.9.1",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3"
  }
}